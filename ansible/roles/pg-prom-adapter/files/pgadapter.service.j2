[Unit]
Description=PG Prometheus Adapter

[Service]
Environment="DATABASE_URL="user=prom password={{ pg_admin_password }} host=c-{{ pg_cluster_id }}.rw.mdb.yandexcloud.net port=6432 database=metrics""
User=andible
Group=ansible
ExecStart=/usr/local/bin/demo
Restart=on-failure

[Install]
WantedBy=multi-user.target